{

  "manifest_version": 2,
  "name": "Pop Firefox Extension",
  "version": "2",

  "description": "Ensures a uniform look for unstyled widgets on webpages in Pop.",

  "icons": {
    "48": "icons/pop-icon-48.png",
    "96": "icons/pop-icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["pop-firefox.css"]
    }
  ]

}
